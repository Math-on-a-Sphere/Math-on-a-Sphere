// This program projects a 20-sided object onto a sphere 
x = 63.5 
theta = 108
thetatwo = 72/2

tri = function () { 
  repeat 3 { forward x right theta }
}

penta = function () {
  repeat 5 { tri() right 72 }
}

repeat 3 {
  penta()
  forward x  
  right thetatwo
}

left 72
forward x
right thetatwo

repeat 3 {
  penta()
  forward x
  left thetatwo
}
